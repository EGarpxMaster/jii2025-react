    "express": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.10.0",
    "pdfkit": "^0.15.2",
    "pino": "^9.3.2",
    "pino-pretty": "^13.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.5.4",
    "@types/pdfkit": "^0.17.3",
